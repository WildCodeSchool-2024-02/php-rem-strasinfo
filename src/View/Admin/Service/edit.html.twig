{% extends "Admin/layout.html.twig" %}

{% block content %}
	<h1>Modifier "{{ service.name}}"</h1>
	{% include "Admin/Service/_form.html.twig" %}
{% endblock %}

{% block javascript %}
	<script>
		$('#summernote').summernote({
placeholder: 'Hello stand alone ui',
tabsize: 2,
height: 120,
toolbar: [
[
'style', ['style']
],
[
'font',
[
'bold', 'underline', 'clear'
]
],
[
'color', ['color']
],
[
'para',
[
'ul', 'ol', 'paragraph'
]
],
[
'table', ['table']
],
[
'insert',
[
'link', 'picture', 'video'
]
],
[
'view',
[
'fullscreen', 'codeview', 'help'
]
]
]
});
	</script>
{% endblock %}
